{
    "manifest_version": 2,
    "name": "PassWorld",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": ["tabs","storage"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
      "default_icon": "images/logo.png",
      "default_popup": "popup/index.html",
      "default_title": "PassWorld!"
    },
    "background": {
      "scripts": ["background.js"]
    }
}
