{
    "manifest_version": 2,
    "name": "PassWorld",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": ["tabs","storage"],
    "browser_action": {
      "default_icon": "images/get_started48.png",
      "default_popup": "popup/index.html",
      "default_title": "PassWorld!"
    }
}
